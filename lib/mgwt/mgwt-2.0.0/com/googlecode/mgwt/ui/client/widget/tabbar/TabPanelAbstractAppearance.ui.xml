<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g='urn:import:com.google.gwt.user.client.ui'
    xmlns:mgwt='urn:import:com.googlecode.mgwt.ui.client.widget'>
    <ui:with field="apperance" type="com.googlecode.mgwt.ui.client.widget.tabbar.TabBarAppearance" />
	<mgwt:panel.flex.FlexPanel ui:field="container" styleName="{apperance.barCss.tabPanel}">
	    <mgwt:carousel.Carousel ui:field="tabContainer" />
	    <mgwt:tabbar.TabPanel.TabBar ui:field="tabBar"/>
	</mgwt:panel.flex.FlexPanel>
</ui:UiBinder>